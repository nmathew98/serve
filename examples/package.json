{
	"name": "sudoku-solver",
	"version": "1.0.0",
	"description": "An example sudoku solver using Serve",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "npx @skulpture/serve build ./postinstall/postinstall.js",
		"lint": "eslint --ext .ts --ignore-path .gitignore . --fix && npx @skulpture/serve typecheck",
		"format": "prettier .  --write",
		"dev": "nodemon -r dotenv/config",
		"start": "node dist/index.js",
		"test": "npx @skulpture/serve jest --coverage --verbose",
		"test.watch": "npx @skulpture/serve jest --watch --coverage --verbose"
	},
	"author": "Naveen Mathew",
	"license": "MIT",
	"devDependencies": {
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^17.0.29",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.79.0",
		"@skulpture/serve": "^4.2.1",
		"bcryptjs": "^2.4.3",
		"busboy": "^1.6.0",
		"dotenv": "^16.0.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.5.0",
		"mongoose": "^6.3.1"
	}
}
