{
	"name": "sudoku-solver",
	"version": "1.0.0",
	"description": "An example sudoku solver using Serve",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "npx @skulpture/serve build ./postinstall/postinstall.js",
		"lint": "eslint --ext .ts --ignore-path .gitignore . --fix && npx @skulpture/serve typecheck",
		"format": "prettier .  --write",
		"dev": "nodemon -r dotenv/config",
		"start": "node dist/index.js",
		"test": "npx @skulpture/serve jest --coverage --verbose",
		"test.watch": "npx @skulpture/serve jest --watch --coverage --verbose"
	},
	"author": "Naveen Mathew",
	"license": "MIT",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/busboy": "^1.5.0",
		"@types/jest": "^27.4.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^17.0.30",
		"@types/passport": "^1.0.7",
		"@types/passport-local": "^1.0.34",
		"@types/uuid": "^8.3.4",
		"@types/uuid-validate": "^0.0.1",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"nodemon": "^2.0.16",
		"prettier": "^2.6.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.80.0",
		"@skulpture/serve": "^5.3.0",
		"bcryptjs": "^2.4.3",
		"busboy": "^1.6.0",
		"dotenv": "^16.0.0",
		"email-validator": "^2.0.4",
		"graphology": "^0.24.1",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.5.0",
		"mongoose": "^6.3.1",
		"passport": "^0.5.2",
		"passport-local": "^1.0.0",
		"password-validator": "^5.2.1",
		"uuid": "^8.3.2",
		"uuid-validate": "^0.0.3"
	}
}
