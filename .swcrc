{
	"minify": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": false,
			"sourceMaps": true
		},
		"target": "es2022",
		"minify": {
			"compress": true,
			"mangle": true
		}
	},
	"module": {
		"type": "commonjs",
		"strictMode": true,
		"lazy": true
	}
}
