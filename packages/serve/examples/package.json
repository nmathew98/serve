{
	"name": "sudoku-solver",
	"version": "1.0.0",
	"description": "An example sudoku solver using Serve",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "npx @skulpture/serve build",
		"lint": "eslint --ext .ts --ignore-path .gitignore . --fix && npx @skulpture/serve typecheck",
		"format": "prettier .  --write",
		"dev": "nodemon -r dotenv/config",
		"start": "node dist/index.js",
		"test": "npx @skulpture/serve jest --coverage --verbose",
		"test.watch": "npx @skulpture/serve jest --watch --coverage --verbose"
	},
	"author": "Naveen Mathew",
	"license": "MIT",
	"devDependencies": {
		"@swc/jest": "^0.2.21",
		"@types/bcryptjs": "^2.4.2",
		"@types/busboy": "^1.5.0",
		"@types/jest": "^28.1.4",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/node": "^18.0.3",
		"@types/passport": "^1.0.9",
		"@types/passport-local": "^1.0.34",
		"@types/uuid": "^8.3.4",
		"@types/uuid-validate": "^0.0.1",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^28.1.2",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.121.0",
		"@sentry/node": "^7.6.0",
		"@sentry/tracing": "^7.6.0",
		"@skulpture/serve": "^6.0.1",
		"bcryptjs": "^2.4.3",
		"busboy": "^1.6.0",
		"email-validator": "^2.0.4",
		"graphology": "^0.24.1",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.7.0",
		"mongoose": "^6.4.4",
		"passport": "^0.6.0",
		"passport-local": "^1.0.0",
		"password-validator": "^5.3.0",
		"uuid": "^8.3.2",
		"uuid-validate": "^0.0.3"
	}
